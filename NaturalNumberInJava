package com.company;

public class naturalNumberProgramInJava
{
    public static void main(String[] args)
    {
        /*
        Write a java program to print Natural Number form
        100 to 200
         */
        System.out.println("This Is Natural Number from 1 to 100");
        int i = 100;
        while(i<=200)
        {
            System.out.println(i);
            i++;
        }
    }
}
